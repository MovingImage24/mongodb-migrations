{
  "name": "mi/mongodb-migrations",
  "description": "Library to migrate forward and backward in mongodb.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "mongodb",
    "migration",
    "puli",
    "plugin"
  ],
  "authors": [
    {
      "name": "Alexander Miehe",
      "email": "alexander.miehe@movingimage.com"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "beta",
  "require": {
    "ext-mongo": "*",
    "symfony/console": "^2.7",
    "symfony/filesystem": "^2.7",
    "zendframework/zend-code": "^3.0",
    "puli/repository": "^1.0@beta",
    "matthiasnoback/symfony-bundle-plugins": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "matthiasnoback/symfony-dependency-injection-test": "^0.7.6",
    "mikey179/vfsStream": "^1.6"

  },
  "autoload": {
    "psr-4": {
      "Mi\\MongoDb\\Migration\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mi\\MongoDb\\Migration\\Tests\\": "tests/"
    }
  }
}
